{
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "npm run generate:css && remix build",
		"deploy": "fly deploy --remote-only",
		"dev:remix": "remix dev",
		"dev:css": "npm run generate:css -- --watch",
		"dev": "npm run build && run-p dev:*",
		"start": "remix-serve build",
		"format": "prettier --write .",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"generate:css": "unocss './app/**/*.{ts,tsx}' -o ./app/styles/uno.css",
		"generate:edgeql": "edgeql-js --target ts",
		"db:migrate": "edgedb migrate && npm run generate:edgeql",
		"db:seed": "npx esbuild-runner dbschema/seed.ts",
		"postinstall": "patch-package"
	},
	"dependencies": {
		"@remix-run/node": "^1.6.7",
		"@remix-run/react": "^1.6.7",
		"@remix-run/serve": "^1.6.7",
		"ariakit": "2.0.0-next.36",
		"edgedb": "^0.21.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"remix-auth": "^3.2.2",
		"remix-auth-github": "^1.1.1"
	},
	"devDependencies": {
		"@iconify/json": "^2.1.85",
		"@remix-run/dev": "^1.6.7",
		"@remix-run/eslint-config": "^1.6.7",
		"@types/react": "^17.0.45",
		"@types/react-dom": "^17.0.17",
		"@unocss/cli": "^0.45.1",
		"@unocss/reset": "^0.45.1",
		"clsx": "^1.2.1",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"npm-run-all": "^4.1.5",
		"patch-package": "^6.4.7",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"typescript": "^4.7.4",
		"unocss": "^0.45.1"
	},
	"engines": {
		"node": ">=14"
	}
}
